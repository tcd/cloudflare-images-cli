{
    "name": "cloudflare-images-cli",
    "version": "0.0.0",
    "description": "A CLI for the Cloudflare Images API.",
    "license": "MIT",
    "repository": "github:tcd/cloudflare-images-cli",
    "bugs": "https://github.com/tcd/cloudflare-images-cli/issues",
    "author": "Clay Dunston <dunstontc@gmail.com>",
    "engines": {
        "node": ">=8.0.0"
    },
    "main": "./bin/main.mjs",
    "bin": {
        "cf-images": "./bin/main.mjs"
    },
    "files": [
        "!src/**/*.ts",
        "./bin/main.mjs",
        "package.json",
        "CHANGELOG.md",
        "LICENSE.md",
        "README.md"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
        "cli": "node ./bin/main.mjs",
        "start": "npm run build && npm run cli"
    },
    "dependencies": {
        "chalk": "^5.0.1",
        "cloudflare-images": "^0.3.0",
        "inquirer": "^9.0.0",
        "meow": "^10.1.3"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.1",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-typescript": "^8.3.3",
        "@types/inquirer": "^8.2.1",
        "@types/node": "^18.0.3",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.19.0",
        "rollup": "^2.76.0",
        "typescript": "^4.7.4"
    },
    "keywords": [
        "cloudflare",
        "Cloudflare",
        "Cloudflare Images",
        "API Client",
        "Assets",
        "cli",
        "CLI",
        "Command Line",
        "Terminal",
        "Tool",
        "Tools"
    ]
}
